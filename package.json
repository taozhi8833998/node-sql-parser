{
  "name": "node-sql-parser",
  "version": "5.4.0",
  "description": "simple node sql parser",
  "main": "index.js",
  "types": "types.d.ts",
  "scripts": {
    "start": "npm run build:pegjs && webpack --config webpack.config.js",
    "build": "npm run lint && npm run build:pegjs && webpack --config webpack.config.js --mode production",
    "build:pegjs": "gulp render && gulp generate",
    "compile": "babel src -d lib",
    "test": "npm run lint && npm run build:pegjs && mochapack --reporter-option maxDiffSize=1000000 \"test/**/*.spec.js\"",
    "lint": "eslint src",
    "coverLocal": "npm run build:pegjs && cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test:only",
    "cover:run": "npm run build:pegjs && cross-env NODE_ENV=coverage nyc --reporter=text-lcov npm run test:only",
    "cover": "npm run cover:run && nyc report --reporter=text-lcov | coveralls",
    "test:only": "mochapack --reporter-option maxDiffSize=1000000 \"test/**/*.spec.js\"",
    "release": "npm run build && npm publish output/prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taozhi8833998/node-sql-parser.git"
  },
  "pre-commit": "coverLocal",
  "keywords": [
    "sql",
    "sql-parser",
    "parser",
    "node",
    "nodejs",
    "node-parser",
    "node-sql-parser",
    "ast",
    "sql-ast"
  ],
  "author": "taozhi8833998 <taozhi8833998@163.com>",
  "files": [
    "ast/",
    "build",
    "lib",
    "umd/",
    "index.d.ts",
    "index.js",
    "index.js.map",
    "LICENSE",
    "package.json",
    "README.md",
    "types.d.ts"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/taozhi8833998/node-sql-parser/issues"
  },
  "homepage": "https://github.com/taozhi8833998/node-sql-parser#readme",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-bugfix-firefox-class-in-computed-class-key": "^7.28.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^10.0.1",
    "babel-loader": "^8.1.0",
    "browserify": "^17.0.0",
    "chai": "^4.4.1",
    "copy-webpack-plugin": "^6.0.3",
    "coveralls": "^3.0.13",
    "cross-env": "^7.0.2",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-strict": "^14.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.16.0",
    "filemanager-webpack-plugin": "^3.1.1",
    "gulp": "^4.0.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^10.0.0",
    "mochapack": "^2.0.3",
    "nyc": "15.1.0",
    "peggy": "^3.0.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.19",
    "tinyify": "^4.0.0",
    "typescript": "^5.3.0",
    "vscode-mocha-hmr": "^1.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "dependencies": {
    "big-integer": "^1.6.48"
  }
}
